{
  "collectionName": "{{models}}",
  "info": {
    "name": "{{model}}",
    "description": "autogenerated model"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    {{#each attributes}}
    "{{name}}": {
        "type": "{{type}}"
    },
    {{/each}}
    "meta": {
        "type": "json"
    }
  }
}
