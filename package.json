{
  "name": "data-driven",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/pro-regular-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "bulma": "^0.8.2",
    "clsx": "^1.1.0",
    "debounce": "^1.2.0",
    "deep-equal": "^2.0.2",
    "immutability-helper": "^3.0.2",
    "merge": "^1.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "uuid": "^7.0.3"
  },
  "scripts": {
    "build:css": "postcss src/assets/app.scss -o src/assets/_app.css",
    "start": "yarn build:css && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pretty": "prettier --single-quote --write \"src/**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md,html,scss}": [
      "prettier --single-quote --write"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^4.0.0",
    "autoprefixer": "^9.7.5",
    "cssnano": "^4.1.10",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.4",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.2"
  }
}
